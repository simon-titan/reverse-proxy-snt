{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*",  
      "destination": "https://snt-starter.webflow.io/:path*"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Host",
          "value": "snt-starter.webflow.io"  
        },
        {
          "key": "Authorization",
          "value": "Basic $(echo -n ':Nuhadt123' | base64)"  }
          
      ]
    }
  ]
}